    <!-- About -->
    <string name="about_title">حول التطبيق</string>
    <string name="app_version">الإصدار 1.0.0</string>
    <string name="about_description_title">وصف التطبيق</string>
    <string name="about_description">H PDF Creator هو تطبيق بسيط وسهل الاستخدام لتحويل الصور إلى ملفات PDF. يمكنك اختيار عدة صور وتحويلها إلى ملف PDF واحد بجودة عالية.</string>
    <string name="about_features_title">المميزات</string>
    <string name="feature_easy_use">سهولة الاستخدام</string>
    <string name="feature_easy_use_desc">واجهة بسيطة وواضحة للاستخدام السهل</string>
    <string name="feature_multiple_images">صور متعددة</string>
    <string name="feature_multiple_images_desc">اختر عدة صور وتحويلها إلى PDF واحد</string>
    <string name="feature_multilingual">دعم اللغات</string>
    <string name="feature_multilingual_desc">دعم كامل للعربية والإنجليزية</string>
    <string name="feature_custom_titles">عناوين مخصصة</string>
    <string name="feature_custom_titles_desc">أضف عنوان مخصص لملف PDF الخاص بك</string>
    <string name="feature_share_print">مشاركة وطباعة</string>
